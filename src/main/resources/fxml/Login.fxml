<StackPane id="LoginPane" fx:id="loginPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="800" prefWidth="480.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celuk.groot.controllers.LoginController">
   <children>
      <VBox fx:id="accessPINVBox" alignment="TOP_CENTER">
         <children>
            <Label fx:id="loginTitle" styleClass="page-title" text="login.title" />
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="panel">
               <children>
                  <Label fx:id="pinLabel" styleClass="pin-label" text="login.pinLabel">
